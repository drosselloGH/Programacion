
package ejercicios_bucles;

import java.util.Scanner;

public class Ejer05b {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.println("Tabla de multiplicar del 6");

		int contador = 0;
		int tablaMultiplicar = 6;

		while (contador <= 10) {
			System.out.println(tablaMultiplicar + " x " + contador + " = " + (contador*tablaMultiplicar));
			contador++;
		}

	}

}
