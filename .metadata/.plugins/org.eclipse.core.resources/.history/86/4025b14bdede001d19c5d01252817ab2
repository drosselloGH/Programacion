	public Ejemplo12MenuBar() {
		super("Barra de men√∫");
		setSize(400, 300);
		setDefaultCloseOperation(EXIT_ON_CLOSE);

		setLayout(new FlowLayout());
		
		JMenu menuArchivo = new JMenu("Archivo");
		
		
		
		

		setVisible(true);

	}

	public static void main(String[] args) {

		new Ejemplo12MenuBar();

	}
}
